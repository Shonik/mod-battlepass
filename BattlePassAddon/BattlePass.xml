                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset>
                                <AbsDimension x="4" y="-2"/>
                            </Offset>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0" right="1" top="0" bottom="1"/>
                </Texture>
                <Texture name="BattlePassFrameWoodBorderTopRight" file="Interface\AchievementFrame\UI-Achievement-WoodBorder-Corner">
