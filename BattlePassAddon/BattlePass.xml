                        <Texture name="BattlePassFrameHeaderLeft" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Size>
                                <AbsDimension x="512" y="106"/>
                            </Size>
                            <Anchors>
                                <Anchor point="BOTTOMLEFT"/>
                            </Anchors>
                            <TexCoords left="0" right="1" top="0" bottom="0.4140625"/>
                        </Texture>
                        <Texture name="BattlePassFrameHeaderRight" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Size>
                                <AbsDimension x="215" y="100"/>
                            </Size>
                            <Anchors>
                                <Anchor point="BOTTOMLEFT" relativeTo="BattlePassFrameHeaderLeft" relativePoint="BOTTOMRIGHT">
                                    <Offset x="0" y="-6"/>
                                </Anchor>
                            </Anchors>
                            <TexCoords left="0" right="0.419921875" top="0.4140625" bottom="0.8046875"/>
                        </Texture>
                    </Layer>
                    <Layer level="BORDER">
                        <Texture name="BattlePassFrameHeaderPointBorder" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Size>
                                <AbsDimension x="133" y="39"/>
                            </Size>
                            <Anchors>
                                <Anchor point="BOTTOM" relativeTo="BattlePassFrameHeader" relativePoint="BOTTOM">
                                    <Offset x="20" y="16"/>
                                </Anchor>
                            </Anchors>
                            <TexCoords left="0.419921875" right="0.6796875" top="0.4140625" bottom="0.56640625"/>
                        </Texture>
                        <FontString name="BattlePassFrameHeaderTitle" inherits="GameFontNormal" text="Battle Pass">
                            <Size x="200" y="12"/>
                            <Anchors>
                                <Anchor point="TOP" relativeTo="BattlePassFrameHeaderPointBorder" relativePoint="TOP">
                                    <Offset x="0" y="7"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                    <Layer level="ARTWORK">
                        <FontString name="BattlePassLevelText" inherits="GameFontHighlightSmall" text="5">
                            <Anchors>
                                <Anchor point="RIGHT" relativeTo="BattlePassFrameHeaderPointBorder" relativePoint="CENTER">
                                    <Offset x="-7" y="0"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                        <FontString name="BattlePassMaxLevelText" inherits="GameFontHighlightSmall" text="/ 100">
                            <Anchors>
                                <Anchor point="LEFT" relativeTo="BattlePassFrameHeaderPointBorder" relativePoint="CENTER">
                                    <Offset x="0" y="0"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
            </Frame>

            <!-- Close Button -->
            <Button name="BattlePassCloseButton" inherits="UIPanelCloseButton">
                <Anchors>
                    <Anchor point="TOPRIGHT">
                        <Offset x="2" y="2"/>
                    </Anchor>
                </Anchors>
            </Button>

            <!-- Progress Bar Frame -->
            <StatusBar name="BattlePassProgressBar" minValue="0" maxValue="100" defaultValue="0" drawLayer="BORDER">
                <Size>
                    <AbsDimension x="800" y="21"/>
                </Size>
                <Anchors>
                    <Anchor point="TOP">
                        <Offset>
                            <AbsDimension x="0" y="-43"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Layers>
                    <Layer level="OVERLAY">
                        <FontString name="BattlePassProgressLabel" inherits="GameFontNormal" text="Progression">
                            <Anchors>
                                <Anchor point="LEFT">
                                    <Offset>
                                        <AbsDimension x="6" y="4"/>
                                    </Offset>
                                </Anchor>
                            </Anchors>
                        </FontString>
                        <Texture name="BattlePassProgressBarLeft" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Size>
                                <AbsDimension x="32" y="48"/>
                            </Size>
                            <Anchors>
                                <Anchor point="TOPLEFT">
                                    <Offset x="-15" y="16"/>
                                </Anchor>
                            </Anchors>
                            <TexCoords left="0.423828125" right="0.486" top="0.56640625" bottom="0.75"/>
                        </Texture>
                        <Texture name="BattlePassProgressBarRight" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Size>
                                <AbsDimension x="32" y="48"/>
                            </Size>
                            <Anchors>
                                <Anchor point="TOPRIGHT">
                                    <Offset x="15" y="16"/>
                                </Anchor>
                            </Anchors>
                            <TexCoords right="0.423828125" left="0.486" bottom="0.75" top="0.56640625"/>
                        </Texture>
                        <Texture name="BattlePassProgressBarMiddle" file="Interface\AchievementFrame\UI-Achievement-Header">
                            <Anchors>
                                <Anchor point="TOPLEFT" relativeTo="BattlePassProgressBarLeft" relativePoint="TOPRIGHT"/>
                                <Anchor point="BOTTOMRIGHT" relativeTo="BattlePassProgressBarRight" relativePoint="BOTTOMLEFT"/>
                            </Anchors>
                            <TexCoords right="0.486" left="0.889224609375" bottom="0.75" top="0.56640625"/>
                        </Texture>
                        <FontString name="BattlePassProgressText" inherits="GameFontHighlight">
                            <Size>
                                <AbsDimension x="0" y="14"/>
                            </Size>
                            <Anchors>
                                <Anchor point="RIGHT">
                                    <Offset x="-5" y="3"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                    <Layer level="BACKGROUND">
                        <Texture name="BattlePassProgressBarBG">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="2" y="-3"/>
                                <Anchor point="BOTTOMRIGHT" x="-2" y="3"/>
                            </Anchors>
                            <Color r="0" g="0" b="0" a="0.5"/>
                        </Texture>
                    </Layer>
                </Layers>
                <BarTexture file="Interface\PaperDollInfoFrame\UI-Character-Skills-Bar"/>
                <BarColor r="0" g="1" b="0"/>
            </StatusBar>

            <!-- Horizontal Scroll Container -->
            <Frame name="BattlePassScrollContainer" enableMouse="true">
                <Size x="830" y="170"/>
                <Anchors>
                    <Anchor point="TOP" x="0" y="-80"/>
                </Anchors>
                <Backdrop edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
                    <BackgroundInsets>
                        <AbsInset left="11" right="12" top="12" bottom="11"/>
                    </BackgroundInsets>
                    <EdgeSize>
                        <AbsValue val="32"/>
                    </EdgeSize>
                </Backdrop>

                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture file="Interface\PaperDollInfoFrame\UI-Character-General-BottomLeft">
                            <Anchors>
                                <Anchor point="TOPLEFT" x="11" y="-12"/>
                                <Anchor point="BOTTOMRIGHT" x="-12" y="11"/>
                            </Anchors>
                            <TexCoords left="0.1" right="0.9" top="0.35" bottom="0.65"/>
                        </Texture>
                    </Layer>
                </Layers>

                <Frames>
                    <!-- Content Frame (where level items go - horizontal layout) -->
                    <Frame name="BattlePassScrollContent">
                        <Size x="820" y="150"/>
                        <Anchors>
                            <Anchor point="CENTER" relativeTo="BattlePassScrollContainer" x="5" y="0"/>
                        </Anchors>
                    </Frame>
                </Frames>

                <Scripts>
                    <OnLoad>
                        self:SetBackdropBorderColor(1, 1, 1, 1)
                    </OnLoad>
                    <OnMouseWheel>
                        BattlePass_OnMouseWheel(self, delta)
                    </OnMouseWheel>
                </Scripts>
            </Frame>

            <!-- Horizontal ScrollBar -->
            <Slider name="BattlePassScrollBar" orientation="HORIZONTAL" enableMouse="true">
                <Size x="820" y="18"/>
                <Anchors>
                    <Anchor point="TOP" relativeTo="BattlePassScrollContainer" relativePoint="BOTTOM">
                        <Offset x="0" y="-5"/>
                    </Anchor>
                </Anchors>
                <Backdrop bgFile="Interface\Buttons\UI-SliderBar-Background" edgeFile="Interface\Buttons\UI-SliderBar-Border" tile="true">
                    <EdgeSize><AbsValue val="8"/></EdgeSize>
                    <TileSize><AbsValue val="8"/></TileSize>
                    <BackgroundInsets><AbsInset left="3" right="3" top="3" bottom="3"/></BackgroundInsets>
                </Backdrop>
                <ThumbTexture name="$parentThumbTexture" file="Interface\Buttons\UI-ScrollBar-Knob">
                    <Size x="24" y="24"/>
                    <TexCoords left="0.20" right="0.80" top="0.125" bottom="0.875"/>
                </ThumbTexture>
                <Scripts>
                    <OnLoad>
                        BattlePassScrollBar_OnLoad(self);
                    </OnLoad>
                    <OnValueChanged>
                        BattlePassScrollBar_OnValueChanged(self, value);
                    </OnValueChanged>
                </Scripts>
            </Slider>

            <!-- Left Arrow Button -->
            <Button name="BattlePassScrollLeftButton">
                <Size x="32" y="32"/>
                <Anchors>
                    <Anchor point="RIGHT" relativeTo="BattlePassScrollBar" relativePoint="LEFT">
                        <Offset x="-3" y="0"/>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        BattlePass_ScrollPrev();
                    </OnClick>
                </Scripts>
                <NormalTexture file="Interface\Buttons\UI-SpellbookIcon-PrevPage-Up"/>
                <PushedTexture file="Interface\Buttons\UI-SpellbookIcon-PrevPage-Down"/>
                <HighlightTexture file="Interface\Buttons\UI-Common-MouseHilight" alphaMode="ADD"/>
            </Button>

            <!-- Right Arrow Button -->
            <Button name="BattlePassScrollRightButton">
                <Size x="32" y="32"/>
                <Anchors>
                    <Anchor point="LEFT" relativeTo="BattlePassScrollBar" relativePoint="RIGHT">
                        <Offset x="3" y="0"/>
                    </Anchor>
                </Anchors>
                <Scripts>
                    <OnClick>
                        BattlePass_ScrollNext();
                    </OnClick>
                </Scripts>
                <NormalTexture file="Interface\Buttons\UI-SpellbookIcon-NextPage-Up"/>
                <PushedTexture file="Interface\Buttons\UI-SpellbookIcon-NextPage-Down"/>
                <HighlightTexture file="Interface\Buttons\UI-Common-MouseHilight" alphaMode="ADD"/>
            </Button>

            <!-- Bottom Buttons -->
            <Button name="BattlePassSyncButton">
                <Size x="158" y="24"/>
                <Anchors>
                    <Anchor point="BOTTOMLEFT" x="40" y="30"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture name="$parentBackground" file="Interface\AchievementFrame\UI-Achievement-Category-Background">
                            <Size x="170" y="32"/>
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                                <Anchor point="TOPRIGHT"/>
                            </Anchors>
                            <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" text="Sync" inherits="GameFontNormalLeftBottom" wordwrap="false" justifyH="CENTER">
                            <Anchors>
                                <Anchor point="CENTER">
                                    <Offset x="0" y="-2"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        BattlePass_RequestSync()
                    </OnClick>
                </Scripts>
                <HighlightTexture file="Interface\AchievementFrame\UI-Achievement-Category-Highlight" alphaMode="ADD">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset x="0" y="0"/>
                        </Anchor>
                        <Anchor point="BOTTOMRIGHT">
                            <Offset x="-1" y="-7"/>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                </HighlightTexture>
            </Button>

            <Button name="BattlePassClaimAllButton">
                <Size x="158" y="24"/>
                <Anchors>
                    <Anchor point="BOTTOM" x="0" y="30"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture name="$parentBackground" file="Interface\AchievementFrame\UI-Achievement-Category-Background">
                            <Size x="170" y="32"/>
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                                <Anchor point="TOPRIGHT"/>
                            </Anchors>
                            <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" text="Claim All" inherits="GameFontNormalLeftBottom" wordwrap="false" justifyH="CENTER">
                            <Anchors>
                                <Anchor point="CENTER">
                                    <Offset x="0" y="-2"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        BattlePass_ClaimAll()
                    </OnClick>
                </Scripts>
                <HighlightTexture file="Interface\AchievementFrame\UI-Achievement-Category-Highlight" alphaMode="ADD">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset x="0" y="0"/>
                        </Anchor>
                        <Anchor point="BOTTOMRIGHT">
                            <Offset x="-1" y="-7"/>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                </HighlightTexture>
            </Button>

            <Button name="BattlePassInfoButton">
                <Size x="158" y="24"/>
                <Anchors>
                    <Anchor point="BOTTOMRIGHT" x="-40" y="30"/>
                </Anchors>
                <Layers>
                    <Layer level="BACKGROUND">
                        <Texture name="$parentBackground" file="Interface\AchievementFrame\UI-Achievement-Category-Background">
                            <Size x="170" y="32"/>
                            <Anchors>
                                <Anchor point="TOPLEFT"/>
                                <Anchor point="TOPRIGHT"/>
                            </Anchors>
                            <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                        </Texture>
                    </Layer>
                    <Layer level="ARTWORK">
                        <FontString name="$parentLabel" text="Info" inherits="GameFontNormalLeftBottom" wordwrap="false" justifyH="CENTER">
                            <Anchors>
                                <Anchor point="CENTER">
                                    <Offset x="0" y="-2"/>
                                </Anchor>
                            </Anchors>
                        </FontString>
                    </Layer>
                </Layers>
                <Scripts>
                    <OnClick>
                        BattlePass_ShowInfo()
                    </OnClick>
                </Scripts>
                <HighlightTexture file="Interface\AchievementFrame\UI-Achievement-Category-Highlight" alphaMode="ADD">
                    <Anchors>
                        <Anchor point="TOPLEFT">
                            <Offset x="0" y="0"/>
                        </Anchor>
                        <Anchor point="BOTTOMRIGHT">
                            <Offset x="-1" y="-7"/>
                        </Anchor>
                    </Anchors>
                    <TexCoords left="0" right="0.6640625" top="0" bottom="1"/>
                </HighlightTexture>
            </Button>
        </Frames>

        <Scripts>
            <OnLoad>
                BattlePass_OnFrameLoad(self)
            </OnLoad>
            <OnDragStart>
                self:StartMoving()
            </OnDragStart>
            <OnDragStop>
                self:StopMovingOrSizing()
            </OnDragStop>
            <OnShow>
                BattlePass_OnShow()
            </OnShow>
        </Scripts>
    </Frame>

