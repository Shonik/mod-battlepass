        <Scripts>
            <OnLoad>
                self:SetBackdropColor(0.1, 0.08, 0.05, 1)
                self:SetBackdropBorderColor(0.6, 0.5, 0.3, 1)
                self.highlight = _G[self:GetName().."Highlight"]
            </OnLoad>
            <OnClick>
                BattlePass_OnLevelItemClick(self)
            </OnClick>
            <OnEnter>
                if self.highlight then
                    self.highlight:Show()
                end
                BattlePass_OnLevelItemEnter(self)
            </OnEnter>
            <OnLeave>
                if self.highlight then
                    self.highlight:Hide()
                end
                GameTooltip:Hide()
            </OnLeave>
        </Scripts>
