        </Layers>
        <Frames>
            <Frame name="BattlePassFrameHeader" enableMouse="true">
                <Size>
                    <AbsDimension x="726" y="106"/>
                </Size>
                <Anchors>
                    <Anchor point="BOTTOM" relativeTo="BattlePassFrame" relativePoint="TOP">
                        <Offset x="0" y="-39"/>
                    </Anchor>
                </Anchors>
