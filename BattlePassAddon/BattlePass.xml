            <EdgeSize>
                <AbsValue val="16"/>
            </EdgeSize>
            <BackgroundInsets>
                <AbsInset left="3" right="3" top="3" bottom="3"/>
            </BackgroundInsets>
        </Backdrop>

        <Layers>
            <Layer level="ARTWORK">
                <FontString name="$parentLevel" inherits="GameFontNormal" justifyH="CENTER" text="1">
                    <Size x="82" y="16"/>
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-5"/>
                    </Anchors>
                    <Color r="1.0" g="0.82" b="0.0"/>
