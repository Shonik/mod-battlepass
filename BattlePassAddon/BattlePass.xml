                <Texture name="BattlePassFrameMetalBorderTopRight" file="Interface\AchievementFrame\UI-Achievement-MetalBorder-Joint">
                    <Size>
                        <AbsDimension x="32" y="32"/>
                    </Size>
                    <Anchors>
                        <Anchor point="TOPRIGHT">
                            <Offset>
