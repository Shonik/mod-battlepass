                    </Shadow>
                </FontString>

                <Texture name="$parentIcon" file="Interface\Icons\INV_Misc_QuestionMark">
                    <Size x="56" y="56"/>
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-28"/>
                    </Anchors>
                </Texture>
            </Layer>

            <Layer level="BORDER">
                <FontString name="$parentName" inherits="GameFontHighlight" justifyH="CENTER" text="Unknown">
                    <Size x="82" y="32"/>
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-88"/>
                    </Anchors>
                </FontString>

                <FontString name="$parentStatus" inherits="GameFontNormalSmall" justifyH="CENTER" text="Locked">
